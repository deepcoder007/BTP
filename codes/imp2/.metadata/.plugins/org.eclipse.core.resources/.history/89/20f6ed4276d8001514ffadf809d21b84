#include"../include/commons.h"
#include<algorithm>

vector<string> split(string str, char delimiter) {
    vector<string> interval;
    stringstream ss(str);
    string tok;
    while( getline(ss,tok,delimiter)) {
        internal.push_back(tok);
    }
    return interval;
}




